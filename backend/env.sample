# Environment
ENVIRONMENT=development

# Database settings
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=newsfeed
POSTGRES_HOST=db

# Casdoor settings
CASDOOR_ENDPOINT=http://casdoor:8000
CASDOOR_CLIENT_ID=your-client-id
CASDOOR_CLIENT_SECRET=your-client-secret
CASDOOR_CERT_PUBLIC_KEY="-----BEGIN CERTIFICATE-----\nMIIE+TCCAuGgAwIBAgIDAeJAMA0GCSqGSIb3DQEBCwUAMDYxHTAbBgNVBAoTFENh\n...\n-----END CERTIFICATE-----"
CASDOOR_ORG_NAME=your-org-name
