# Example environment variables for NewsFeed

# --- Next.js Frontend ---
NEXTAUTH_URL=https://newsfeed.example.com
NEXTAUTH_SECRET=your-nextauth-secret
CASDOOR_ENDPOINT=https://casdoor.example.com
CASDOOR_CLIENT_ID=your-casdoor-client-id
CASDOOR_CLIENT_SECRET=your-casdoor-client-secret
CASDOOR_CERTIFICATE=your-casdoor-certificate
BASE_DOMAIN=example.com

# --- FastAPI Backend ---
REDIS_URL=redis://redis:6379/0
FRESHRSS_URL=http://freshrss:80
FRESHRSS_API_USER=your-freshrss-user
FRESHRSS_API_PASSWORD=your-freshrss-password
OLLAMA_URL=http://ollama:11434
OLLAMA_MODEL=llama2

#Plausible
NEXT_PUBLIC_PLAUSIBLE_ENABLE=true
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=newsfeed.example.com
NEXT_PUBLIC_PLAUSIBLE_URL=plausible.example.com